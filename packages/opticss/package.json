{
  "name": "opticss",
  "version": "0.1.0",
  "description": "A CSS Optimizer",
  "main": "dist/src/index.js",
  "scripts": {
    "compile": "rm -rf dist && tsc --pretty -p test/tsconfig.json && cp test/util/*.js dist/test/util",
    "pretest": "npm run compile && tslint --project tsconfig.json --type-check",
    "prepublish": "tsc -p tsconfig.json",
    "tslint": "tslint --project tsconfig.json",
    "test": "mocha dist/test --opts test/mocha.opts -i --grep Integration",
    "integration": "npm run pretest && mocha dist/test --opts test/mocha.opts --grep Integration",
    "coverage": "istanbul cover -i \"dist/src/**/*.js\" --dir ./build/coverage node_modules/.bin/_mocha -- dist/test --opts test/mocha.opts",
    "remap": "remap-istanbul -i build/coverage/coverage.json -o coverage -t html",
    "docs": "typedoc --out ./docs .",
    "grammars": "nearleyc test/util/attrvalue.ne -o test/util/attrvalue.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/css-blocks/opticss.git"
  },
  "keywords": [
    "css",
    "optimizer"
  ],
  "author": "Chris Eppstein",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/css-blocks/opticss/issues"
  },
  "types": "dist/src/index.d.ts",
  "homepage": "https://github.com/css-blocks/opticss#readme",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/nearley": "^2.9.0",
    "@types/node": "^7.0.39",
    "@types/random-js": "^1.0.30",
    "chai": "^3.5.0",
    "css-select": "^1.3.0-rc0",
    "css-size": "^2.0.0",
    "grunt": "^1.0.1",
    "grunt-release": "^0.14.0",
    "grunt-run": "^0.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.0.23",
    "moo": "^0.4.1",
    "nearley": "^2.10.3",
    "parse5": "^3.0.2",
    "perfectionist": "^2.4.0",
    "random-js": "^1.0.8",
    "remap-istanbul": "^0.9.5",
    "resolve-cascade": "^0.1.0",
    "source-map-support": "^0.4.17",
    "tslint": "^5.1.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "@types/debug": "0.0.29",
    "@types/node": "^8.0.26",
    "@types/source-map": "^0.5.0",
    "concat-with-sourcemaps": "^1.0.4",
    "css-property-parser": "^0.1.0",
    "debug": "^2.6.8",
    "inline-source-map-comment": "^1.0.5",
    "object.values": "^1.0.4",
    "postcss": "^6.0.12",
    "postcss-selector-parser": "^2.2.3",
    "regexpu-core": "^4.0.11",
    "source-map": "^0.5.6",
    "specificity": "^0.3.2",
    "typescript-collections": "^1.2.3",
    "typescript-memoize": "^1.0.0-alpha.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "engineStrict": true
}
