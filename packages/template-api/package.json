{
  "name": "@opticss/template-api",
  "version": "0.1.0",
  "description": "Simple templates for use in testing with OptiCSS.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "compile": "rm -rf dist test/build && tsc && tsc -p tsconfig-test.json",
    "pretest": "yarn run compile && yarn run tslint",
    "prepublish": "rm -rf dist && tsc",
    "tslint": "tslint --project tsconfig.json && tslint --project tsconfig-test.json",
    "test": "mocha test/build/test --opts test/mocha.opts",
    "coverage": "istanbul cover -i \"dist/src/**/*.js\" --dir ./build/coverage node_modules/.bin/_mocha -- dist/test --opts test/mocha.opts",
    "remap": "remap-istanbul -i build/coverage/coverage.json -o coverage -t html",
    "docs": "typedoc --out ./docs ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/css-blocks/opticss.git"
  },
  "keywords": [
    "css",
    "cascade"
  ],
  "author": "Chris Eppstein",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/css-blocks/opticss/issues"
  },
  "homepage": "https://github.com/css-blocks/opticss#readme",
  "peerDependencies": {
    "opticss": "*"
  },
  "dependencies": {
    "@opticss/util": "*",
    "postcss": "^6.0.12",
    "typescript-collections": "^1.2.3"
  },
  "devDependencies": {
    "@types/node": "^8.0.26",
    "@opticss/attr-analysis-dsl": "^0.1.0",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.1.9",
    "tslint": "^5.1.0",
    "typescript": "^2.4.1"
  }
}
